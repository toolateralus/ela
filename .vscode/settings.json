{
  "lldb.showDisassembly": "auto",
  "lldb.dereferencePointers": true,
  "lldb.consoleMode": "commands",
  "files.associations": {
    "*.ela": "jai"
  },

  "files.exclude": {
    "**/.clock_diff.txt": true,
    // we hide these cause they're ugly.
    "**/bin": true,
    "**/objs": true,
  
    // For windows users, you may want to remove these.
    "**/*vcxproj": true,
    "**/*vcxproj.filters": true,
    "**/*vcxproj.user": true,
    "**/*.sln": true,
    "**/*.cache": true
  }
}

