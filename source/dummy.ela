#import core;

Vec2 :: struct {
  f32 x;
  f32 y;
  #ctor :: (f32 _x, f32 _y) {
    x = _x;
    y = _y;
  }
  to_string :: () -> char * {
    char* arr = malloc(sizeof(char) * 256);
    snprintf(arr, 256, "x=%f, y=%f", x, y);
    return arr;
  }
}

main :: () {
  n := #make(Vec2, 0, 0);
  printf("%s\n", n.to_string());
}