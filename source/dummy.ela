
Vector2 :: struct {
  f32 x;
  f32 y;
  
  print :: () {
    printf("x: %f, y: %f\n", x, y);
  }
  
  zero :: () -> Vector2 {
    Vector2 v;
    return v;
  };
  
  #ctor :: () {}
  
  #ctor :: (f32 _x, f32 _y) {
    x = _x;
    y = _y;
  }

  #dtor :: () {
    
  }
  
}

main :: () {
  Vector2 v = #make(1.0, 1.0);
  printf("%f\n", v.x);
}
