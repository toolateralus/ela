
#alias char :: u32; // this represents a single UTF-8 scalar.

string :: struct {
  data:    u8*,
  length:  u64,
}

Collection_Initializer :: struct![T] {
  data:   T*,
  length: u64,
}

List :: struct![T] {
  data:     T*,
  length:   u64,
  capacity: u64,
}

Type :: struct;
any :: struct {
  ptr: void*,
  type: Type*
};
