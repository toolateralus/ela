import json::*;
import fs::*;

struct JsonValueDeleter {
  vals: List!<*const JsonValue> = List!<*const JsonValue>,
}

impl JsonValueDeleter {
  fn add(*mut self, val: *const JsonValue) {
    self.vals.push(val);
  }

  fn destroy(*mut self) {
    for val in self.vals {
      std::c::free(val);
    }
    self.vals.destroy();
  }
}