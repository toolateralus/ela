;

IHaveAMethod :: struct {}

impl IHaveAMethod {
  method :: fn(self*) -> float32 {
    return 0.0;
  }
  parameter_method :: fn(self*, x: float32) -> float32 {
      return x;
  }
}

#test test_method :: fn() {
  owner: IHaveAMethod ;
  assert("failed to get 0.0 from method."c, owner.method() == 0.0);
  assert("failed to get 0.0 from method (passing back param)."c, owner.parameter_method(0.0) == 0.0);
}
