
filter :: fn![T](array: List![T]*, func: Func![T, bool]) -> List![T] {
  output: List![T];
  for v in array {
    if func(v) {
      output.push(v);
    }
  }
  return output;
}


main :: fn() {
  array: List![s32] = {0,1,2,3};

  array = filter(array, fn(v: s32) -> bool {
    return v > 1;
  });

  // prints {2, 3};
  println(array);
}