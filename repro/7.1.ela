#import core;

/* 
  ! Referencing a type that doesn't exist just causes a freaking segfault in the compiler.
  ! In this case, Color isn't defined.
*/

Wall :: struct {
  length: int;
  height: int;
}

WorldBuilder :: struct {
  walls: Wall*[];
  wall :: (length: int, height)  {
    walls ~= new Wall;
  }
} 
