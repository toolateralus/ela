#import dlfcn;
#import core;

// Not a compile time error but a runtime error.
// This some_function type will equal 'void()*', the parameters just get completely discarded.

#alias some_function :: void(int)*;

main :: () {
  Type *type = #type some_function;
  if strcmp(type.name, "void(int)*") != 0 {
    println($"expected void(int)*, got {type.name}");
  }
}
