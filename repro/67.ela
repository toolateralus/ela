main :: fn() { 
  // we need to handle operator overloads in switch statements.
  // should be relatively straightforward, just have an ASTBinExpr on the stack in the emitter & typer.
  // boom.
  s: str = "Hello!";
  passed = false;
  switch s {
    str::from("Hello"): { assert("Switch case on str failed", false); }
    str::from("Hello!"): { passed = true; }
  }
}
