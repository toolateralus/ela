#foreign memmove :: (dest: void*, src: void*, nbytes: s64) -> int;

insert_at :: (arr: s64[]*, v: s64, idx: s64) {
  if idx < 0 || idx > arr.length {
    return;
  }

  (*arr) ~= 0;

  memmove(
    arr.data + idx + 1, //! BUG::: The pointer arithmetic here crashes the compiler
    arr.data + idx,
    (arr.length - idx - 1) * sizeof(s64)
  );

  (*arr)[idx] = v;
}

