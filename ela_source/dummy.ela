thing :: struct {
  a: s32;
  b: s32;
}

#foreign printf :: fn(c: c_string, ...) -> int;

impl thing {
  static_method :: fn(a: s32, b: s32) -> s32 {
    return a + b;
  }
  mut_me :: fn(self*) {
    printf("Hi from a '%s'\n", $"{#type(self).name}".data);
    self.a = 100;
    self.b = 200;
  }
}

main :: fn() {
  t: thing = {
    0, 0
  };
  n := t.static_method(5, 2);
  t.mut_me();
  printf("%d, %d\n", t.a, t.b);
}