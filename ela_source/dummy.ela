add :: (a: s32, b: s32) -> s32 { return a + b; }
add :: (a: float, b: s32) -> s32 { return ((s32)a) + b; }
add :: (a: s32, b: float) -> s32 { return a + ((s32)b); }
add :: (a: s32, b: u32) -> s32 { return a + ((s32)b); }
add :: (a: s32) -> s32 { return a + 2; }
add :: (a: s32, b: s32, c: s32) -> s32 { return a + b + c; }

