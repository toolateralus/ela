#foreign strlen :: fn(str: c_string) -> int;

#static split :: fn(str: c_string, delimiter: char) -> string[] {
  if !str then return {};
  result: string[];
  start: int = 0;
  str_len: int = strlen(str);
  for i in 0..str_len {
    if str[i] == delimiter {
      if i > start 
        then result ~= string {str + start, str + i};
      start = i + 1;
    }
  }
  result ~= string {str + start, str + str_len};
}

