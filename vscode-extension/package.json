{
  "name": "ela",
  "displayName": "ELA",
  "description": "Syntax highlighting for ELA language",
  "version": "0.0.1",
  "publisher": "toolateralus",
  "engines": {
    "vscode": "^1.95.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "ela",
        "aliases": ["ELA", "ela"],
        "extensions": [".ela"],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "ela",
        "scopeName": "source.ela",
        "path": "./syntaxes/ela.tmLanguage.json"
      }
    ]
  }
}